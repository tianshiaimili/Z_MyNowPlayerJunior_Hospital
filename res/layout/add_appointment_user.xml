<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:clipChildren="false"
    tools:context=".TestMainActivity"
    tools:ignore="HardcodedText,DisableBaselineAlignment,ContentDescription" >
	<include layout="@layout/tabbar_head"/>
	
	 <LinearLayout
        android:id="@+id/adduserDialogLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" 
        android:layout_below="@layout/tabbar_head"
        android:layout_marginTop="50dp"
        >

         <TextView
        android:id="@+id/add_U_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:singleLine="true"
        android:text="姓名 :"
        android:textColor="#575757"
        android:textSize="19sp" />
        
        <EditText
            android:id="@+id/add_U_name_editT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请输入您的姓名"
             />

        <TextView
        android:id="@+id/add_U_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:singleLine="true"
        android:text="性别 :"
        android:textColor="#575757"
        android:textSize="19sp" />
        
        <RadioGroup 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/radiogroup_background"
            >
            
            <RadioButton
                android:id="@+id/man_radioB" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="男"
        android:textSize="19sp"
        android:background="@color/radiobutton_man_background"
        android:textColor="@android:color/black"    
        android:gravity="center" 
        android:button="@null"
		android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:checked="true"
        
                />
             <RadioButton 
                 android:id="@+id/woman_radioB" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="女"
        android:textSize="19sp"
		android:background="@color/radiobutton_woman_background"
        android:textColor="@android:color/black"    
        android:gravity="center" 
        android:button="@null"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
                />
            
        </RadioGroup>
        <!-- 生日 -->
        <TextView
        android:id="@+id/add_U_birthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:singleLine="true"
        android:text="生日 :"
        android:textColor="#575757"
        android:textSize="19sp" />
        
                <EditText
            android:id="@+id/add_U_date_TextV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请选择出生日期"
            android:inputType="date"
            />
        
        
        <Button
            android:id="@+id/comfirtButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingLeft="50dp"
            android:paddingRight="50dp"
            android:text="确认添加"
            android:textSize="19sp"
            android:background="@color/button_selector"
             />
    </LinearLayout>
	
	
